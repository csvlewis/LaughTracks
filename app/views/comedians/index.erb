<% @comedians.each do |comedian| %>
  <ul>
    <li><%= comedian.name %></li>
    <li><%= comedian.age %></li>
    <li><%= comedian.city %></li>
    <% comedian.specials.each do |special| %>
      <li><%= special.title  %><li>
      <li><%= special.run_time %> minutes<li>
      <img src="<%= special.image_url %>" alt=" <%= special.title %>" height="150" width="100">
    <% end %>
  </ul>
<% end %>
